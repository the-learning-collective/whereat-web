{
  "name": "whereat-web",
  "version": "1.0.0",
  "description": "location sharing app for activists",
  "engines": {
    "node": "0.12.x",
    "npm": "2.11.x"
  },
  "main": "main.jsx",
  "scripts": {
    "dev": "npm run dev-serve | npm run dev-build",
    "dev-build": "webpack-dev-server --devtool eval --progress --colors --hot --content-base build --port 8090 --config webpack_dev_config",
    "dev-serve": "http-server -p 8080 ./build/dev",
    "build": "UV_THREADPOOL_SIZE=128 NODE_ENV=production webpack -p --config webpack_prod_config.js --progress --display-error-details --display-chunks",
    "test": "NODE_ENV=test mocha src/test/**/*.spec.js --compilers js:babel/register --require=src/test/support/dom.js",
    "test-coverage": "istanbul cover _mocha src/test/**/*.spec.js --ui -R  -t=5000 -- --compilers=js:babel-core/register --require=src/test/support/dom.js",
    "test-debug": "node-inspector --web-port=9000 & mocha src/test/**/*.spec.js --compilers js:babel/register --debug-brk"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:the-learning-collective/whereat-web.git"
  },
  "keywords": [
    "mobile",
    "web",
    "location",
    "socialJustice"
  ],
  "author": "@aguestuser",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/the-learning-collective/whereat-web/issues"
  },
  "homepage": "https://github.com/the-learning-collective/whereat-web",
  "dependencies": {
    "babel": "^5.6.23",
    "babel-core": "^5.4.7",
    "babel-loader": "^5.1.3",
    "babel-runtime": "^5.7.0",
    "bulk-require": "^0.2.1",
    "classname": "0.0.0",
    "classnames": "^2.1.2",
    "css-loader": "^0.13.1",
    "expose-loader": "^0.7.0",
    "file-loader": "^0.8.3",
    "fs": "0.0.2",
    "html-webpack-plugin": "^1.4.0",
    "image-webpack-loader": "^1.4.0",
    "imagemin": "^3.2.0",
    "immutable": "^3.7.3",
    "imports-loader": "^0.6.4",
    "istanbul": "^0.3.18",
    "js-nacl": "^0.6.0",
    "jsx-loader": "^0.13.2",
    "leaflet": "^0.7.3",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "lodash": "^3.10.1",
    "marty": "^0.10.1",
    "node-libs-browser": "^0.5.0",
    "node-sass": "^3.1.2",
    "node-uuid": "^1.4.3",
    "postcss-loader": "^0.5.1",
    "pretty-date": "^0.2.0",
    "react": "^0.13.3",
    "react-bootstrap": "^0.22.6",
    "react-google-maps": "^1.7.1",
    "react-hot-loader": "^1.2.7",
    "react-leaflet": "^0.6.2",
    "react-tappable": "^0.5.7",
    "style-loader": "^0.12.2",
    "superagent": "^1.3.0",
    "url-loader": "^0.5.5",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.8.2",
    "xml-loader": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^2.3.0",
    "chai-as-promised": "^5.0.0",
    "http-server": "^0.8.0",
    "jsdom": "^3.1.2",
    "mocha": "^2.2.5",
    "mocha-babel": "^3.0.0",
    "mocha-istanbul": "^0.2.0",
    "react-test-tree": "^0.2.2",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0",
    "superagent-mock": "^1.3.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      "lessify",
      "bulkify"
    ]
  }
}
